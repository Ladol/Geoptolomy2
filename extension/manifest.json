{
    "manifest_version": 3,
    "name": "GeoPhotoService Network Listener",
    "version": "1.0",
    "permissions": ["webRequest", "webRequestBlocking", "storage", "activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "host_permissions": [
      "*://maps.googleapis.com/*",
      "*://geotastic.net/*"
    ]
  }
  